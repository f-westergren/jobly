{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "title": "The root schema",
  "description": "The root schema comprises the entire JSON document.",
  "default": {},
  "examples": [
      {
          "password": "folke1",
          "first_name": "Folke",
          "last_name": "Filbyte",
          "email": "folke@gmail.com",
          "photo_url": "https://www.photo.com",
          "is_admin": true
      }
  ],
  "properties": {
      "password": {
          "$id": "#/properties/password",
          "type": "string",
          "minLength": 1,
          "title": "The password schema",
          "description": "An explanation about the purpose of this instance.",
          "default": "",
          "examples": [
              "folke1"
          ]
      },
      "first_name": {
          "$id": "#/properties/first_name",
          "type": "string",
          "minLength": 1,
          "title": "The first_name schema",
          "description": "An explanation about the purpose of this instance.",
          "default": "",
          "examples": [
              "Folke"
          ]
      },
      "last_name": {
          "$id": "#/properties/last_name",
          "type": "string",
          "minLength": 1,
          "title": "The last_name schema",
          "description": "An explanation about the purpose of this instance.",
          "default": "",
          "examples": [
              "Filbyte"
          ]
      },
      "email": {
          "$id": "#/properties/email",
          "type": "string",
          "minLength": 1,
          "title": "The email schema",
          "description": "An explanation about the purpose of this instance.",
          "default": "",
          "examples": [
              "folke@gmail.com"
          ]
      },
      "photo_url": {
          "$id": "#/properties/photo_url",
          "type": "string",
          "title": "The photo_url schema",
          "description": "An explanation about the purpose of this instance.",
          "default": "",
          "examples": [
              "https://www.photo.com"
          ]
      },
      "is_admin": {
          "$id": "#/properties/is_admin",
          "type": "boolean",
          "title": "The is_admin schema",
          "description": "An explanation about the purpose of this instance.",
          "default": false,
          "examples": [
              true
          ]
      },
      "_token": {
        "$id": "/properties/token",
        "type": "string",
        "title": "The Token Schema ",
        "default": "",
        "examples": [
          "foo123"
        ]
      }
  },
  "additionalProperties": false
}